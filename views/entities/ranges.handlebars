
<form class="form-horizontal">
  <fieldset>
    <!-- Form Name -->
    <legend>Enter or search Range</legend>

    <!-- Text input-->
    <div class="form-group">
      <input
        id="name"
        name="name"
        type="text"
        placeholder="Range Name"
        required=""
      />
    </div>

    <!-- Text input-->
    <div class="form-group">
      <input
        id="description"
        name="description"
        type="text"
        placeholder="Range Description"
        required=""
      />
    </div>

    <br />
    <button type="button" onclick="">Enter</button>
    <button type="button" onclick="">Search</button>
  </fieldset>
</form>

<table id="rangesData">
  <thead>
    <tr>
      <th>ID</th>
      <th>Range Name</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>❌ 1</td>
      <td>✍ Normal</td>
      <td>Effects one target Pokemon</td>
    </tr>

    <!-- Add more rows as needed -->
  </tbody>
</table>
